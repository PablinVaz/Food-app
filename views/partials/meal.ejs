<main>           
    <% for(let i = 0; i < meals.length; i++) {%>
    <article>           
        <img src="<%= meals[i].img_url%>" alt="<%= meals[i].img_alt%>">
        <div class="plato-info">
            <p class="precio"><%= meals[i].price%> &euro;</p>
            <h5><%= meals[i].name%></h5>
            <p class="descripcion-plato"><%= meals[i].description%></p>
        </div>
        <div class="contadores">
            <button class="contador suma">&#43;</button>
            <p class="conta-cantidad">0</p>
            <button class="contador resta">&#45;</button>
        </div>
    </article>
    <% } %>            
</main>